<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="597.0" prefWidth="909.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="storebolt.CheckOutController">
   <children>
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="355.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/logoo.png" />
         </image>
      </ImageView>
      <TableView fx:id="orderTable" layoutX="186.0" layoutY="99.0" prefHeight="375.0" prefWidth="530.0" />
      <VBox alignment="CENTER" layoutX="727.0" layoutY="241.0" prefHeight="92.0" prefWidth="180.0">
         <children>
            <Label text="Total:">
               <font>
                  <Font size="24.0" />
               </font></Label>
            <Label fx:id="totalPrice" alignment="CENTER" contentDisplay="CENTER" prefHeight="35.0" prefWidth="102.0" text="Label">
               <font>
                  <Font size="24.0" />
               </font></Label>
         </children>
      </VBox>
      <Button fx:id="orderButton" layoutX="743.0" layoutY="375.0" mnemonicParsing="false" onAction="#placeOrder" prefHeight="39.0" prefWidth="106.0" style="-fx-background-radius: 20; -fx-background-insets: 0; -fx-background-color: #40434a;" text="Place Order" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="deleteButton" layoutX="555.0" layoutY="522.0" mnemonicParsing="false" onAction="#deleteOrder" prefHeight="39.0" prefWidth="106.0" style="-fx-background-radius: 20; -fx-background-insets: 0; -fx-background-color: #40434a;" text="Delete" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="backButton" layoutX="249.0" layoutY="522.0" mnemonicParsing="false" onAction="#backToHome" prefHeight="39.0" prefWidth="106.0" style="-fx-background-radius: 20; -fx-background-insets: 0; -fx-background-color: #40434a;" text="Back" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
